<%= render "users/admin" %>
<div class="panelbox">
  <div class="panel panel-default p_t_30">
    <div class="panel-heading"><h2>Malls</h2></div>
    <div class="panel-body">
      <table class="table table-striped table table-bordered">
        <tr>
          <th>name</th>
          <th>address</th>
          <th>city</th>
        </tr>
      <% @malls.each do |m|%>
        <tr>
          <td><%= m.name%></td>
          <td><%= m.address%></td>
          <td><%=m.city.name%><br /></td> 
        </tr>
      <% end%>
      </table>
    </div>
  </div>
</div>
