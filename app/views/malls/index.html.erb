
<center>
<h2> List of all users </h2><br /><br />
  <div class="table table-striped">
	<table border="2">
		<tr>
		 <th>name</th>
		 <th>address</th>
		 <th>city</th>
		</tr>
		<% @mall.each do |m|%>
		<tr>
		<td><%= m.name%></td>
		<td><%= m.address%></td>
		<td><%=m.city_id%><br /></td>	
		</tr>
		<% end%>
	</table>
  </div>
</center>
