
<center>
<h2> List of all users </h2><br /><br />
  <div class="table table-striped">
  <table border="2">
    <tr>
     <th>name</th>
     <th>email</th>
     <th>action</th>
    </tr>
    <% @users.each do |u|%>
    <tr>
    <td><%= u.first_name%></td>
    <td><%= u.email%></td>
    <td><%=link_to "Delete" ,user_path(u.id), :method => "delete" %><br /></td> 
    </tr>
    <% end%>
  </table>
  </div>
  
</center>

<div style="position:absolute;top:45px;right:20px;">
<%=link_to "sign out" ,destroy_user_session_path, :method => "delete" %><br/>
<%=link_to "Back" , users_path %>
</div>
